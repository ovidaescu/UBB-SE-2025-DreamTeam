<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="Marketplace_SE.PlacedOrderPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Marketplace_SE"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel>
            <Button Content="Return to account" 
            HorizontalAlignment="Center" 
            VerticalAlignment="Center" 
            Width="150" 
            Height="50" 
            Click="OnButtonClickNavigatePlacedOrderPageAccountPage"
            x:Name="ButtonNavigatePlacedOrderPageAccountPage"/>
        </StackPanel>
    </Grid>
</Page>
